<h1>Google Search Console</h1>

<%= form_with url: search_console_analytics_path, method: :post do |form| %>
  <div>
    <%= form.label :site_url %>
    <%= form.text_field :site_url %>
  </div>
  <div>
    <%= form.label :start_date %>
    <%= form.date_field :start_date %>
  </div>
  <div>
    <%= form.label :end_date %>
    <%= form.date_field :end_date %>
  </div>
  <div>
    <%= form.submit "Fetch Analytics" %>
  </div>
<% end %>
